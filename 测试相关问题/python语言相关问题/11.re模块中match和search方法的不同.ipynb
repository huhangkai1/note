   "source": [
    "# re模块中match和search方法的不同\n",
    "* match在字符串开始位置匹配\n",
    "* search扫描整个字符串查找匹配"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "<re.Match object; span=(0, 5), match='hello'>\n",
      "<re.Match object; span=(0, 5), match='hello'>\n",
      "None\n",
      "<re.Match object; span=(5, 10), match='world'>\n",
      "hello\n"
     ]
    }
   ],
   "source": [
    "import re\n",
    "s1 = 'helloworld, helloworld'\n",
    "print(re.match('hello',s1))\n",
    "print(re.search('hello',s1))\n",
    "print(re.match('world',s1))\n",
    "print(re.search('world',s1))\n",
    "print(re.match('hello',s1).group())"
   ]
  }
 ],
