   "source": [
    "# 字符串的拆分方法\n",
    "* string对象的split方法,不允许多个分割符\n",
    "* 函数re.split()，允许多个分割符"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['dsfsda,sdf', 'sdfsda*dsf', 'df']\n",
      "['dsfsda', 'sdf', 'sdfsda', 'dsf', 'df']\n",
      "['ds', 'da', 'sdf', 'sd', 'da', 'dsf', 'df']\n"
     ]
    }
   ],
   "source": [
    "line = \"dsfsda,sdf sdfsda*dsf df\"\n",
    "print(line.split(\" \"))\n",
    "import re\n",
    "print(re.split('[ ,*]',line))\n",
    "print(re.split(' |,|\\*|fs',line))"
   ]
  }
 ],
